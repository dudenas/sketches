{
  "manifest_version": 2,
  "name": "murama art",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["libraries/p5.js", "libraries/p5.dom.js", "content.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "muramart.png"
  }
}