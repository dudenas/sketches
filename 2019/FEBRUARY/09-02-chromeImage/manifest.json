{
  "manifest_version": 2,
  "name": "murama art",
  "version": "0.1",
  "web_accessible_resources": [
    "img/*.png"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "muramart.png"
  }
}